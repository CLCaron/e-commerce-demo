{
  "name": "e-commerce-demo-server",
  "version": "1.0.0",
  "engines": {
    "node": "18.12.1",
    "npm": "6.14.13"
  },
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js"
  },
  "dependencies": {
    "all": "^0.0.0",
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "dotenv": "7.0.0",
    "express": "^4.16.4",
    "mongodb": "^6.2.0",
    "mongoose": "^6.0.5",
    "stripe": "6.28.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.19.1"
  }
}
